<div id="<%= dom_id servico %>">
  <p>
    <strong>Nome:</strong>
    <%= servico.nome %>
  </p>

  <p>
    <strong>Descricao:</strong>
    <%= servico.descricao %>
  </p>

  <p>
    <strong>Data:</strong>
    <%= servico.data %>
  </p>

  <p>
    <strong>Horario inicio:</strong>
    <%= servico.horario_inicio %>
  </p>

  <p>
    <strong>Horario temino:</strong>
    <%= servico.horario_temino %>
  </p>

  <p>
    <strong>Preco:</strong>
    <%= servico.preco %>
  </p>

  <p>
    <strong>Trabalhador:</strong>
    <%= Trabalhador.find(servico.trabalhador_id).nome_completo %>
  </p>

</div>
